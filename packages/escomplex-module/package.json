{
  "name": "@ponticus/escomplex-module",
  "version": "0.1.0",
  "description": "Provides module / individual file oriented AST processing for typhonjs-escomplex complexity reports.",
  "homepage": "https://github.com/typhonjs-node-escomplex/@ponticus/escomplex-module",
  "license": "MPL-2.0",
  "author": {
    "name": "Michael Leahy"
  },
  "main": "./dist",
  "repository": {
    "type": "git",
    "url": "https://github.com/typhonjs-node-escomplex/@ponticus/escomplex-module.git"
  },
  "dependencies": {
    "escomplex-plugin-metrics-module": "^0.1.0",
    "escomplex-plugin-syntax-babylon": "^0.1.0",
    "@ponticus/ast-walker": "^0.2.0",
    "@ponticus/escomplex-commons": "^0.1.0",
    "typhonjs-plugin-manager": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@ponticus/babel-parser": "^0.2.0",
    "acorn": "^6.0.0",
    "babylon": "^6.0.0",
    "espree": "^5.0.0",
    "esprima": "^4.0.0",
    "eslint": "^8.18.0",
    "eslint-plugin-prettier": "^4.1.0",
    "@babel/preset-env": "^7.18.6",
    "@babel/register": "^7.18.6",
    "mocha": "^10.0.0"
  },
  "scripts": {
    "build": "node ../npm-scripts-build-babel/scripts/build.js",
    "build-syntax": "babel ./node_modules/@ponticus/escomplex-commons/src -d ./node_modules/@ponticus/escomplex-commons/dist && babel ./node_modules/escomplex-plugin-metrics-module/src -d ./node_modules/escomplex-plugin-metrics-module/dist && babel ./node_modules/escomplex-plugin-syntax-estree/src -d ./node_modules/escomplex-plugin-syntax-estree/dist && babel ./node_modules/escomplex-plugin-syntax-babylon/src -d ./node_modules/escomplex-plugin-syntax-babylon/dist && babel ./node_modules/typhonjs-plugin-manager/src -d ./node_modules/typhonjs-plugin-manager/dist",
    "lint": "eslint .",
    "prepublish": "babel-node ./node_modules/typhonjs-npm-scripts-publish/scripts/prepublish.js",
    "test": "mocha --require @babel/register test/src/*.js",
    "test-coverage": "babel-node ./node_modules/typhonjs-npm-scripts-test-mocha/scripts/mocha-istanbul.js",
    "dev-test": "BABEL_ENV=escomplex-dev babel-node ./node_modules/typhonjs-npm-scripts-test-mocha/scripts/mocha.js dev_test",
    "dev-test-coverage": "BABEL_ENV=escomplex-dev babel-node ./node_modules/typhonjs-npm-scripts-test-mocha/scripts/mocha-istanbul.js dev_test"
  },
  "keywords": [
    "typhonjs",
    "escomplex",
    "module",
    "complexity",
    "simplicity",
    "cyclomatic",
    "halstead",
    "maintainability",
    "dependencies",
    "static",
    "analysis",
    "metrics",
    "es6",
    "es7",
    "es8",
    "es9",
    "es10"
  ],
  "volta": {
    "node": "16.15.0"
  }
}

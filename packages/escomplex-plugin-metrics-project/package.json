{
  "name": "@ponticus/escomplex-plugin-metrics-project",
  "version": "0.1.0",
  "homepage": "https://github.com/typhonjs-node-escomplex/@ponticus/escomplex-plugin-metrics-project/",
  "description": "Provides the core project metric / report generation plugin for typhonjs-escomplex project processing.",
  "license": "MPL-2.0",
  "main": "./dist/PluginMetricsProject",
  "repository": {
    "type": "git",
    "url": "https://github.com/typhonjs-node-escomplex/@ponticus/escomplex-plugin-metrics-project.git"
  },
  "dependencies": {
    "@ponticus/escomplex-commons": "^0.1.0"
  },
  "devDependencies": {
    "typhonjs-config-eslint": "^0.5.0",
    "@ponticus/escomplex-test-data": "0.0.1",
    "@babel/preset-env": "^7.18.6",
    "@babel/register": "^7.18.6",
    "mocha": "^10.0.0"
  },
  "scripts": {
    "build": "node ../npm-scripts-build-babel/scripts/build.js",
    "esdoc": "esdoc -c .esdocrc",
    "eslint": "eslint .",
    "prepublish": "babel-node ./node_modules/typhonjs-npm-scripts-publish/scripts/prepublish.js",
    "test": "mocha --require @babel/register test/src/*.js",
    "test-coverage": "babel-node ./node_modules/typhonjs-npm-scripts-test-mocha/scripts/mocha-istanbul.js",
    "dev-test": "BABEL_ENV=escomplex-dev babel-node ./node_modules/typhonjs-npm-scripts-test-mocha/scripts/mocha.js dev_test",
    "dev-test-coverage": "BABEL_ENV=escomplex-dev babel-node ./node_modules/typhonjs-npm-scripts-test-mocha/scripts/mocha-istanbul.js dev_test"
  },
  "keywords": [
    "typhonjs",
    "escomplex",
    "plugin",
    "metrics",
    "project"
  ],
  "volta": {
    "node": "16.15.0"
  }
}

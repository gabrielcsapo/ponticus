{
  "name": "@ponticus/ast-walker",
  "version": "0.2.1",
  "homepage": "https://github.com/ponticus/ast-walker/",
  "description": "Provides a simple Javascript AST traversal utility that traverses all nodes / children regardless of type.",
  "main": "./dist",
  "repository": {
    "type": "git",
    "url": "https://github.com/typhonjs-node-ast/@ponticus/ast-walker.git"
  },
  "devDependencies": {
    "@ponticus/npm-scripts-build-babel": "^0.7.1",
    "mocha": "^10.0.0",
    "typhonjs-config-eslint": "^0.4.0"
  },
  "scripts": {
    "build": "node ../npm-scripts-build-babel/scripts/build.js",
    "esdoc": "esdoc -c .esdocrc",
    "eslint": "eslint .",
    "prepublish": "babel-node ./node_modules/typhonjs-npm-scripts-publish/scripts/prepublish.js",
    "test": "mocha test/src/**/*.js",
    "test-coverage": "babel-node ./node_modules/typhonjs-npm-scripts-test-mocha/scripts/mocha-istanbul.js"
  },
  "keywords": [
    "typhonjs",
    "ast",
    "traversal"
  ],
  "files": [
    "dist",
    "src",
    "AUTHORS.md"
  ],
  "bugs": {
    "url": "https://github.com/typhonjs-node-ast/@ponticus/ast-walker/issues"
  },
  "volta": {
    "node": "16.15.0"
  }
}

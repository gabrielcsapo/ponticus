{
  "name": "@ponticus/plugin-manager",
  "version": "0.0.1",
  "description": "Provides a plugin manager that dispatches events to loaded plugins.",
  "homepage": "https://github.com/gabrielcsapo/ponticus",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielcsapo/ponticus.git"
  },
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/dts/index.d.ts",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "vitest": "^0.24.5"
  },
  "scripts": {
    "build": "swc src -d dist && tsc",
    "lint": "eslint .",
    "prepublish": "npm run build",

{
  "name": "@ponticus/babel-parser",
  "version": "0.0.1",
  "description": "Provides a front end for Javascript / Typescript AST generation by Babel parser with TyphonJS plugin support.",
  "homepage": "https://github.com/gabrielcsapo/ponticus",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielcsapo/ponticus.git"
  },
  "type": "module",
  "scripts": {
    "build": "swc src -d dist && tsc",
    "lint": "eslint .",
    "prepublish": "npm run build",
    "test": "vitest"
  },
  "keywords": [
    "babel",
    "parser",
    "AST"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/dts/index.d.ts",
  "dependencies": {
    "@babel/parser": "^7.20.3"
  },
  "devDependencies": {
    "@babel/types": "^7.20.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14",
    "@ponticus/plugin-manager": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "vitest": "^0.25.1"
  },
  "volta": {
    "node": "16.15.0"
  }
}

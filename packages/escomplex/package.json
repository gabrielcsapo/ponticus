{
  "name": "@ponticus/escomplex",
  "version": "0.0.0",
  "description": "Next generation complexity reporting for Javascript & Typescript based on the Babel parser.",
  "homepage": "https://github.com/typhonjs-node-escomplex/typhonjs-escomplex",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./cjs/index.js"
  },
  "dependencies": {
    "@ponticus/babel-parser": "^0.0.0",
    "@ponticus/escomplex-module": "^0.0.0",
    "@ponticus/escomplex-project": "^0.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.207",
    "@swc/jest": "^0.2.21",
    "eslint": "^8.18.0",
    "eslint-plugin-prettier": "^4.1.0",
    "@ponticus/escomplex-test-data": "0.0.0",
    "vitest": "^0.16.0"
  },
  "scripts": {
    "build": "swc src -d dist && swc src -d cjs --config module.type=commonjs",
    "lint": "eslint .",
    "prepublish": "npm run build",
    "test": "vitest"
  },
  "keywords": [
    "typhonjs",
    "escomplex",
    "complexity",
    "simplicity",
    "cyclomatic",
    "halstead",
    "maintainability",
    "dependencies",
    "static",
    "analysis",
    "metrics",
    "babylon",
    "es6",
    "es7",
    "es8",
    "es9",
    "es10"
  ],
  "volta": {
    "node": "16.15.0"
  }
}
